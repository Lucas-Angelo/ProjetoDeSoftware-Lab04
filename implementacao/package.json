{
  "name": "lab4",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "undomigrations": "npx sequelize-cli db:migrate:undo:all --env 'development'",
    "migrations": "npx sequelize-cli db:migrate --env 'development'",
    "seeds": "npx sequelize-cli db:seed:all --env 'development'",
    "config": "npm run undomigrations && npm run migrations && npm run seeds",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "api": "nodemon ./api/index"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "4.17.1",
    "express-async-errors": "^3.1.1",
    "mysql2": "^2.3.0",
    "nuxt": "latest",
    "sequelize": "^6.6.5",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@types/cors": "^2.8.12",
    "nodemon": "^2.0.12",
    "sequelize-cli": "^6.3.0"
  }
}
